public class CalculatorEx {
	public static void main(String[] args) {
		System.out.println("덧셈 : 5 + 3 = " + Calculator.sum(5, 3));
		System.out.println("뺄셈 : 9 - 7 = " + Calculator.sub(9, 7));
		System.out.println("곱셈 : 8 * 4 = " + Calculator.mul(8, 4));
		System.out.println("나눗셈 : 12 / 6 = " + Calculator.div(12, 6));
	}
}
