class Calculator {
	public static int sum(int x, int y) {
		int sum = x + y;
		
		return sum;
	}
	
	public static int sub(int x, int y) {
		int sub = x - y;
				
		return sub;
	}
	
	public static int mul(int x, int y) {
		int mul = x * y;
		
		return mul;
	}
	
	public static int div(int x, int y) {
		int div = x / y;
		
		return div;
	}
}
